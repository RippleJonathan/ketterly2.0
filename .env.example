# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Email (Resend)
# Get your API key from: https://resend.com/api-keys
# NOTE: In development, Resend only allows sending to your own verified email
# To send to customers, verify a domain at: https://resend.com/domains
RESEND_API_KEY=re_your_api_key_here
RESEND_FROM_EMAIL=orders@ketterly.com

# SMS (Twilio) - Optional
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=

# Google Maps API - For auto-measuring roofs from satellite imagery
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Enable the Solar API: https://console.cloud.google.com/apis/library/solar.googleapis.com
GOOGLE_MAPS_API_KEY=your_google_maps_api_key
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=AIzaSyDzSAkpy7rvhiwc5HePEk3lySVB-TG3Pak

# Property Data API (for homeowner lookup during door knocking)
# Option 1: Melissa Data Property API - https://www.melissa.com/developer/property
# Cost: ~$0.02-0.05 per lookup, easy setup
MELISSA_API_KEY=your_melissa_api_key

# Option 2: Attom Data Solutions - https://api.attomdata.com/
# Cost: Subscription-based (~$100-500/month), more comprehensive data
# ATTOM_API_KEY=your_attom_api_key

# OneSignal - Push Notifications
# Get your credentials from: https://onesignal.com
# App ID is public (used in browser), REST API Key is private (server-side only)
NEXT_PUBLIC_ONESIGNAL_APP_ID=your_onesignal_app_id
ONESIGNAL_REST_API_KEY=your_onesignal_rest_api_key